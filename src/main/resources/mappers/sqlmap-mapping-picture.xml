<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.tian.zone.dao.picture.PictureDAO">
	
	<select id="getPictures" resultType="com.tian.zone.dto.picture.PictureDTO">
		select * from coffee_picture
	</select>
	<select id="getPicturesById" parameterType="java.lang.String" resultType="com.tian.zone.dto.picture.PictureDTO">
		select p.* from coffee_picture p where p.user_id = #{id}
	</select>
</mapper>